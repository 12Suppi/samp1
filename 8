#include <iostream>
using namespace std;
class Base {
protected:
int alpha;
public:
Base(int a) {
alpha = a;
cout << "Base class constructor called" << endl;
}
};
class Derived : public Base {
protected:
int beta;
int gamma;
public:
Derived(int a, int b, int c) {
beta = b;
gamma = c;
cout << "Derived class constructor called" << endl;
}
void displayValues() {
cout << "Alpha: " << alpha << endl;
cout << "Beta: " << beta << endl;
cout << "Gamma: " << gamma << endl;};
int main() {
Derived obj(1, 2, 3);
obj.displayValues();
return 0;